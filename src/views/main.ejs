
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
<!-- <link rel="stylesheet" href="./main.css" /> -->

<main>

  <div class="card" style="width: 18rem;">
    <% places.forEach((place) => { %>
      <img src="<%= place.link %>" alt="<%= place.contenido %>" class="card-img-top" >
    <div class="card-body">
      <h5 class="card-title"><%= place.titulo %></h5>
      <p class="card-text"><%= place.contenido %></p>
      <p class="card-text"><%= place.createdAt %></p>
      <a type="button" class="btn btn-warning" href="/updates/<%= place.id %>">Editar</a>
      <a type="button" class="btn btn-danger" href="/delete/<%= place.id %>">Eliminar</a>
      <% }); %>
    </div>   
  
</main>


</div>
